it('should emit scanner input event if observers exist', () => {
  const input = 'XYZ789';
  
  // Subscribe to the observable to simulate observers
  service.getScannerObservable().subscribe();

  // Spy on the next() method of the observable
  const nextSpy = jest.spyOn(service['scannerInputEventObservable'], 'next');
  
  // Call finalizeScannerInput
  service['finalizeScannerInput'](input);
  
  // Expect next() to be called with the correct input data
  expect(nextSpy).toHaveBeenCalledWith({
    scannerInput: input,
    isTag: false
  });
});